{
  "name": "stimulus-rails-nested-form",
  "version": "0.1.0",
  "description": "A Stimulus controller to create a checkbox tree for your forms.",
  "repository": "git@github.com:coderberry/stimulus-checkbox-tree.git",
  "author": "Eric Berry <eric@berry.sh>",
  "license": "MIT",
  "homepage": "https://github.com/coderberry/stimulus-checkbox-tree",
  "private": false,
  "main": "dist/stimulus-checkbox-tree.umd.js",
  "module": "dist/stimulus-checkbox-tree.mjs",
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
    "lint": "prettier-standard  '**/*.{ts,css,html}' --lint",
    "dev": "vite",
    "prod": "vite build --mode netlify",
    "build": "tsc --noEmit && vite build",
    "version": "yarn build",
    "test": "BABEL_ENV=test jest",
    "np": "np --no-2fa"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.2.1",
    "@testing-library/dom": "^8.19.1",
    "@types/jest": "^29.2.4",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mutationobserver-shim": "^0.3.7",
    "np": "^7.6.2",
    "postcss": "^8.4.20",
    "prettier-standard": "16.4.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/jest-setup.js"
    ]
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.2.1"
  }
}
